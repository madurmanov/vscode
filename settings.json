{
  "commentTranslate.targetLanguage": "ru",
  "editor.cursorStyle": "block",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Meslo LG M DZ', Consolas, 'Courier New', monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "trailing",
  "editor.tabSize": 2,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "html.format.wrapLineLength": 0,
  "liveServer.settings.donotShowInfoMsg": true,
  "notebook.output.wordWrap": true,
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "workbench.colorTheme": "Ayu Light Bordered",
  "workbench.editorAssociations": {
    "*.{md,csv,svg}": "default",
    "{git,gitlens,git-graph}:/**/*.{md,csv,svg}": "default"
  },
  "vim.useSystemClipboard": true,
  "vim.smartRelativeLine": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 999,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["J"],
      "after": ["5", "j"]
    },
    {
      "before": ["K"],
      "after": ["5", "k"]
    },

    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["Q"],
      "after": ["<C-w>", "q"]
    },

    {
      "before": ["|"],
      "after": ["<C-w>", "v"]
    },
    {
      "before": ["_"],
      "after": ["<C-w>", "s"]
    },

    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.focusRightGroup"]
    },

    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },

    {
      "before": ["<leader>", "g", "p"],
      "commands": ["editor.action.dirtydiff.next"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "g", "j"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["<leader>", "g", "k"],
      "commands": ["workbench.action.editor.previousChange"]
    },

    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    },

    {
      "before": ["<leader>", "t", "t"],
      "commands": [":tabnew"]
    },
    {
      "before": ["<leader>", "t", "o"],
      "commands": [":tabo"]
    },

    {
      "before": ["<leader>", "k", "k"],
      "commands": ["editor.fold"]
    },
    {
      "before": ["<leader>", "k", "l"],
      "commands": ["editor.unfold"]
    },
    {
      "before": ["<leader>", "k", "0"],
      "commands": ["editor.foldAll"]
    },
    {
      "before": ["<leader>", "k", "l", "0"],
      "commands": ["editor.unfoldAll"]
    },
    {
      "before": ["<leader>", "k", "="],
      "commands": ["editor.foldAllBlockComments"]
    },
    {
      "before": ["<leader>", "k", "1"],
      "commands": ["editor.foldLevel1"]
    },
    {
      "before": ["<leader>", "k", "2"],
      "commands": ["editor.foldLevel2"]
    },
    {
      "before": ["<leader>", "k", "3"],
      "commands": ["editor.foldLevel3"]
    },
    {
      "before": ["<leader>", "k", "4"],
      "commands": ["editor.foldLevel4"]
    },
    {
      "before": ["<leader>", "k", "5"],
      "commands": ["editor.foldLevel5"]
    },
    {
      "before": ["<leader>", "k", "6"],
      "commands": ["editor.foldLevel6"]
    },
    {
      "before": ["<leader>", "k", "7"],
      "commands": ["editor.foldLevel7"]
    },

    {
      "before": ["<leader>", ";"],
      "commands": [":nohls"]
    },

    {
      "before": ["<leader>", "m", "m"],
      "commands": ["office.markdown.switch"]
    },
    {
      "before": ["<leader>", "m", "h"],
      "commands": ["office.html.preview"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "."],
      "commands": ["editor.action.blockComment"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    },

    {
      "before": ["<leader>", "t"],
      "commands": ["commentTranslate.replaceSelections"]
    },

    {
      "before": ["<leader>", "k", "-"],
      "commands": ["editor.foldAllExcept"]
    },
    {
      "before": ["<leader>", "k", "l", "-"],
      "commands": ["editor.unfoldAllExcept"]
    }
  ],
  "workbench.colorCustomizations": {
    "[Ayu*]": {
      "editorBracketHighlight.foreground1": "#2D2D2D",
      "editorBracketHighlight.foreground2": "#2D2D2D",
      "editorBracketHighlight.foreground3": "#2D2D2D",
      "editorBracketHighlight.foreground4": "#2D2D2D",
      "editorBracketHighlight.foreground5": "#2D2D2D",
      "editorBracketHighlight.foreground6": "#2D2D2D",
      "foreground": "#2D2D2D",
      "list.inactiveSelectionForeground": "#2D2D2D",
      "sideBarSectionHeader.foreground": "#2D2D2D",
      "statusBar.foreground": "#2D2D2D",
      "tab.activeForeground": "#2D2D2D",
      "tab.inactiveForeground": "#2D2D2D",

      "activityBar.background": "#FEF8E7",
      "debugToolBar.background": "#FEF8E7",
      "dropdown.background": "#FEF8E7",
      "editor.background": "#FEF8E7",
      "editor.lineHighlightBackground": "#F9EDDB",
      "editorBracketMatch.background": "#FFDD99",
      "editorGroupHeader.tabsBackground": "#FEF8E7",
      "editorHoverWidget.background": "#FEF8E7",
      "editorSuggestWidget.background": "#FEF8E7",
      "editorSuggestWidget.selectedBackground": "#F9EDDB",
      "editorWidget.background": "#FEF8E7",
      "input.background": "#FEF8E7",
      "list.activeSelectionBackground": "#F9EDDB",
      "list.focusBackground": "#F9EDDB",
      "list.hoverBackground": "#F9EDDB",
      "list.inactiveSelectionBackground": "#F9EDDB",
      "minimap.background": "#FEF8E7",
      "notificationCenterHeader.background": "#FEF8E7",
      "notifications.background": "#FEF8E7",
      "panel.background": "#FEF8E7",
      "scrollbarSlider.activeBackground": "#0087D840",
      "scrollbarSlider.background": "#0087D820",
      "scrollbarSlider.hoverBackground": "#0087D830",
      "sideBar.background": "#FEF8E7",
      "sideBarSectionHeader.background": "#FEF8E7",
      "statusBar.background": "#FEF8E7",
      "statusBar.noFolderBackground": "#FEF8E7",
      "tab.activeBackground": "#FEF8E7",
      "tab.inactiveBackground": "#FEF8E7",
      "terminal.background": "#FEF8E7",
      "titleBar.activeBackground": "#FEF8E7",
      "titleBar.inactiveBackground": "#FEF8E7"
    }
  },
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  }
}
